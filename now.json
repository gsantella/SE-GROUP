{
    "name": "se-group",
    "version": 2,
    "builds": [
      { "src": "src/backend/server.js", "use": "@now/node" },
      { "src": "package.json", "use": "@now/static-build" }
    ],
    "routes": [
      { "src": "/api", "dest": "src/backend/server.js" },
      { "src": "/(.*)", "dest": "frontend/$1" }
    ],
    "regions": ["cle1"]
}